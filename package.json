{
    "name": "supabase-solid-chat",
    "version": "0.0.0",
    "description": "",
    "scripts": {
        "start": "vite --port 6006",
        "dev": "npm-run-all --parallel start typesafe-i18n",
        "build": "vite build",
        "type-check": "tsc --project tsconfig.json --pretty --noEmit",
        "lint": "prettier --check . --ignore-path .gitignore && eslint .",
        "format": "prettier --ignore-path .gitignore --write .",
        "format-check": "prettier -- --list-different",
        "validate": "npm-run-all --parallel type-check lint build",
        "typesafe-i18n": "typesafe-i18n",
        "cypress:open": "cypress open"
    },
    "license": "MIT",
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^5.35.1",
        "@typescript-eslint/parser": "^5.35.1",
        "cypress": "^10.6.0",
        "cypress-dark": "^1.8.3",
        "cypress-vite": "^1.1.1",
        "eslint": "^8.22.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-cypress": "^2.12.1",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jsx-a11y": "^6.6.1",
        "eslint-plugin-prettier": "^4.2.1",
        "husky": "^8.0.1",
        "lint-staged": "^13.0.3",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.7.1",
        "typescript": "^4.7.4",
        "vite": "^3.0.9",
        "vite-plugin-solid": "^2.3.0"
    },
    "dependencies": {
        "@felte/solid": "^1.2.3",
        "@felte/validator-yup": "^1.0.9",
        "@hope-ui/solid": "^0.6.7",
        "@solidjs/router": "^0.4.2",
        "@stitches/core": "^1.2.8",
        "@supabase/supabase-js": "^2.0.0-rc.6",
        "solid-hcaptcha": "^0.2.5",
        "solid-js": "^1.4.8",
        "solid-transition-group": "^0.0.10",
        "typesafe-i18n": "^5.12.0",
        "yup": "^0.32.11"
    },
    "husky": {
        "hooks": {
            "pre-commit": "CI=1 npm-run-all --parallel type-check && lint-staged"
        }
    },
    "lint-staged": {
        "*.+(js|ts|tsx)": [
            "yarn run lint"
        ],
        "*.+(js|json|ts|tsx)": [
            "yarn run format"
        ]
    },
    "packageManager": "pnpm@7.9.5"
}
