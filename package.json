{
    "name": "supabase-solid-chat",
    "version": "0.0.0",
    "description": "",
    "scripts": {
        "start": "vite",
        "dev": "npm-run-all --parallel start typesafe-i18n",
        "build": "vite build",
        "type-check": "tsc --project tsconfig.json --pretty --noEmit",
        "lint": "prettier --check . --ignore-path .gitignore && eslint .",
        "format": "prettier --ignore-path .gitignore --write .",
        "format-check": "prettier -- --list-different",
        "validate": "npm-run-all --parallel type-check lint build",
        "typesafe-i18n": "typesafe-i18n"
    },
    "license": "MIT",
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "4.33.0",
        "@typescript-eslint/parser": "4.33.0",
        "autoprefixer": "10.4.8",
        "babel-preset-solid": "1.5.1",
        "eslint": "7.32.0",
        "eslint-config-prettier": "8.5.0",
        "eslint-plugin-import": "2.26.0",
        "eslint-plugin-jsx-a11y": "6.6.1",
        "eslint-plugin-prettier": "3.4.1",
        "husky": "8.0.1",
        "lint-staged": "13.0.3",
        "npm-run-all": "4.1.5",
        "prettier": "2.7.1",
        "solid-transition-group": "0.0.8",
        "typescript": "4.8.2",
        "vite": "2.9.15",
        "vite-plugin-solid": "2.3.0"
    },
    "dependencies": {
        "@felte/solid": "1.2.3",
        "@felte/validator-yup": "1.0.9",
        "@fontsource/work-sans": "4.5.11",
        "@hope-ui/solid": "0.6.7",
        "@solidjs/router": "0.4.2",
        "@supabase/supabase-js": "2.0.0-rc.3",
        "solid-hcaptcha": "0.2.5",
        "solid-js": "1.5.1",
        "typesafe-i18n": "5.12.0",
        "yup": "0.32.11"
    },
    "husky": {
        "hooks": {
            "pre-commit": "CI=1 npm-run-all --parallel type-check && lint-staged"
        }
    },
    "lint-staged": {
        "*.+(js|ts|tsx)": [
            "yarn run lint"
        ],
        "*.+(js|json|ts|tsx)": [
            "yarn run format"
        ]
    }
}
